AAA,CAApC,C,CARJ,C;GASK,CATL,E;;AAAA,GAUC,EAXL,CAyBE,EAbF,YAAA,CAYO,MAZP,EAYO;AAZA,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,OAAA,EAAV;AAAA,KAAA,EAAiB,CAAA,SAAA,CAAjB,CAAA;AAYN,GAZP,E,iBACI,YAAA,CAIE,OAJF,EAIE;AAHE,IAAA,KAAK,EAAC,8DAGR;;aAFW,KAAA,CAAA,OAAA,GAAO,M;MAElB;AADE,IAAA,WAAW,EAAC;AACd,GAJF,E,IAAA,E;;AAAA,G,iBAEa,KAAA,CAAA,O,KAGb,U,CANJ,E;;AAAA,GAaE,CA3BN,C","sourcesContent":["<template>\n    <div class=\"m-auto\">\n        <h1 class=\"text-center text-2xl\">Marvel Heroes {{ heroCount }}</h1>\n        <ul class=\"my-5\">\n            <li\n                class=\"flex justify-between py-1\"\n                v-for=\"(hero, index) in marvelHero\"\n                :key=\"hero.name\"\n            >\n                <div>\n                    {{ hero.name }}\n                </div>\n                <button @click=\"removeHero(index)\">x</button>\n            </li>\n        </ul>\n        <form @submit.prevent=\"addHero()\">\n            <input\n                class=\"bg-gray-200 rounded text-gray-800 p-2 border-gray-400 border\"\n                v-model=\"newHero\"\n                placeholder=\"Type name hero here\"\n            />\n            <button\n                type=\"submit\"\n                class=\"bg-blue-600 text-white rounded px-1 py-2\"\n            >\n                Add Hero\n            </button>\n        </form>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            newHero: \"\",\n            marvelHero: [\n                { name: \"Captain America\" },\n                { name: \"Iron Man\" },\n                { name: \"Hulk\" },\n                { name: \"Black Widow\" },\n            ],\n        };\n    },\n\n    methods: {\n        addHero() {\n            if (this.newHero !== \"\") {\n                // check if the input text is not empty\n                this.marvelHero.unshift({ name: this.newHero }); // push to objek data marvelHero at the start\n                this.newHero = \"\"; // clear the input text after submit\n            }\n        },\n\n        removeHero(index) {\n            this.marvelHero = this.marvelHero.filter((hero, i) => i != index);\n        },\n    },\n\n    computed: {\n        heroCount() {\n            return this.marvelHero.length;\n        },\n    },\n};\n</script>"],"sourceRoot":""}]}