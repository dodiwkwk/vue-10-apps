,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,OAAA,EAAV;AAAA,KAAA,EAAiB,CAAA,SAAA,CAAjB,CAAA;AAGN,GAHP,E,iBACI,YAAA,CAA6D,OAA7D,EAA6D;;aAA7C,KAAA,CAAA,OAAA,GAAO,M;MAAsC;AAApC,IAAA,WAAW,EAAC;AAAwB,GAA7D,E,IAAA,E;;AAAA,G,iBAAgB,KAAA,CAAA,O,KAChB,U,CAFJ,E;;AAAA,G","sourcesContent":["<template>\n    <h1>Marvel Heroes {{ heroCount }}</h1>\n    <ul>\n        <li v-for=\"(hero, index) in marvelHero\" :key=\"hero.name\">\n            <div>\n                {{ hero.name }} <button @click=\"removeHero(index)\">x</button>\n            </div>\n            <input />\n        </li>\n    </ul>\n    <form @submit.prevent=\"addHero()\">\n        <input v-model=\"newHero\" placeholder=\"Type name hero here\" />\n        <button type=\"submit\">Add Hero</button>\n    </form>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            newHero: \"\",\n            marvelHero: [\n                { name: \"Captain America\" },\n                { name: \"Iron Man\" },\n                { name: \"Hulk\" },\n                { name: \"Black Widow\" },\n            ],\n        };\n    },\n\n    methods: {\n        addHero() {\n            if (this.newHero !== \"\") {\n                // check if the input text is not empty\n                this.marvelHero.unshift({ name: this.newHero }); // push to objek data marvelHero at the start\n                this.newHero = \"\"; // clear the input text after submit\n            }\n        },\n\n        removeHero(index) {\n            this.marvelHero = this.marvelHero.filter((hero, i) => i != index);\n        },\n    },\n\n    computed: {\n        heroCount() {\n            return this.marvelHero.length;\n        },\n    },\n};\n</script>"],"sourceRoot":""}]}